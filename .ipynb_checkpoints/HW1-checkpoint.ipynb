    "import numpy as np\n",
    "\n",
    "\n",
    "def obtain_lucas_numbers(n):\n",
    "    L = [1, 3]\n",
    "\n",
    "    for i in range(2, n):\n",
    "        L.append(L[i-1] + L[i-2])\n",
    "\n",
    "    return L"
   ]
